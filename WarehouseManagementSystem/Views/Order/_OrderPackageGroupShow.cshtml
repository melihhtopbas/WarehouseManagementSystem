@model List<Warehouse.ViewModels.Admin.OrderPackageListViewModel>
<table id="showOrderPackageGroup" class="table table-bordered">
    <thead>
        <tr>

            <th>Ağırlık(kg)</th>
            <th>En(cm)</th>
            <th>Yükselik(cm)</th>
            <th>Boy(cm)</th>
            <th>Desi</th>
            <th>Ürünler</th>





        </tr>
    </thead>
    <tbody id="OrderPackageList">
        @foreach (var item in Model)
        {
            <tr>

                <td>@item.Weight kg</td>
                <td>@item.Width cm</td>
                <td>@item.Height cm</td>
                <td>@item.Length cm</td>
                <td>@item.Desi</td>
                <td>
                    @foreach (var packageProduct in item.OrderPackageProductGroups)
                    {
                        <span>@packageProduct.Count</span><span>x</span>
                        <span>@packageProduct.Content</span>
                        <br />
                    }
            </td>
        </tr>
    }


    </tbody>


</table>
<div class="clearfix"></div>
  
     @foreach (var prdtc in ViewData["ProductList"] as List<Warehouse.ViewModels.Admin.ProductGroupShowViewModel>)
     {
         <span>@prdtc.Content üründen <strong>@prdtc.Count</strong> adetinden <strong>@prdtc.isPackagedCount</strong> tane paketlenmeyen vardır. </span>
         <br />
     }
   
<div class="clearfix"></div>


<div class="form-group form-group-sm row">
    <div class="col-xs-12">

        <button class="btn btn-danger pull-right" type="button" data-dismiss="modal"><span class="fa fa-times"></span>&nbsp;&nbsp;Kapat</button>
    </div>
</div>
