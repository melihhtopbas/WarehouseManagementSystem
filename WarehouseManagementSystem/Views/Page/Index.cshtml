@{
    string className = "filter1";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model Warehouse.ViewModels.WebSite.PageDetailViewModel
<!--/END TOP HEADER AREA-->




<div class="details-banner">
    <div class="content d-flex align-items-center justify-content-center text-center">
        <div class="container">
            <div class="breadcrumb-holder d-inline-flex">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a href="@Url.Action("Index","Home")">
                                @WebSiteViewItems.HomePage
                                <i class="fa fa-angle-double-right" aria-hidden="true"></i>
                            </a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="#">
                                @WebSiteViewItems.Corporate
                                <i class="fa fa-angle-double-right" aria-hidden="true"></i>
                            </a>
                        </li>
                        <li class="breadcrumb-item" aria-current="page">@Model.Name</li>
                    </ol>
                </nav>
            </div>
            <!-- End of .breadcrumb-holder -->
            <h1>@Model.Name</h1>
        </div>
        <!-- End of .container -->
    </div>
    <!-- End of .content -->
</div>




<section class="section terms-privacy">
    <div class="container">
        <div class="terms-privacy-content">
            <div class="content">



                <div class="content-block">
                    <h3>@Model.Name</h3>
                    <p>
                        @Html.Raw(Model.Description)
                    </p>
                </div>
                <!-- End of .content-block -->
                <!-- End of .content-block -->
            </div>
            <!-- End of .content -->
        </div>
        <!-- End of .terms-privacy-content -->
    </div>
    <!-- End of .container -->
</section>
<!-- End of .case-studies -->
