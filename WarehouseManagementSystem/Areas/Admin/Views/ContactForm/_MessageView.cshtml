
@{
    Layout = null;
}
@model Warehouse.ViewModels.Admin.ContactViewModel

<div class="row">
    <div class="box box-primary">


        <div class="box-body no-padding">
            <div class="mailbox-read-info">
                <h3>@Model.Subject</h3>
                <h5>
                    Ad: @Model.FullName<br />
                    Telefon: @Model.Phone<br />
                    Mail: @Model.Email<br />
                    <span class="mailbox-read-time pull-right">@Model.Date</span>
                </h5>
            </div>

            <div class="mailbox-read-message">
                @Model.Message
            </div>

        </div>

    </div>
</div>