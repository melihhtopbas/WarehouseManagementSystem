
@model Warehouse.ViewModels.Admin.CargoServiceTypeListViewModel
<tr id="@("cargoService_"+Model.Id)" class="master-row">

    <td>@Model.Id</td>
    <td>
        @Model.Name
    </td>
    <td style="width:50px">
        <a data-modal-form="true"
           data-modal-size="modal-lg"
           data-modal-update="#cargoService_@Model.Id"
           data-modal-highlight="#cargoService_@Model.Id"
           data-modal-title="Kargo Düzenle"
           data-hook-beforeunload="true"
           class="btn btn-block btn-success btn-sm"
           href="@Url.Action("Edit", "CargoServiceSetting", routeValues: new {Area = "Admin",cargoServiceId=Model.Id})">
            <i class="fa fa-edit fa-fw text-success"></i> Kargo Düzenle
        </a>

    </td>
    <td style="width:50px">
        <a href="javascript:void(0);"
           data-toggle="confirmation"
           title="Emin Misiniz"
           data-btn-ok-label="Evet"
           data-btn-cancel-label="Hayır"
           data-placement="top"
           data-cargo-service-id="@Model.Id"
           data-on-confirm="deleteCargoService"
           class="btn btn-block btn-danger btn-sm">
            <i class="fa fa-trash fa-fw text-danger"></i> Kargo Sil
        </a>

    </td>
</tr>
